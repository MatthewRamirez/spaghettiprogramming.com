<h2>Sign In</h2>
<%= form_for :user, :url => sessions_path do |f| %>
  <div class="field">
    <%= f.label :email %>
  </div>
  <div class="field">
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %>
  </div>
  <div class="field">
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit :submit %>
  </div>
<% end %>
